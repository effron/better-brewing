<canvas id="tasting-chart-<%= tasting_note.id %>" width="900" height="900"></canvas>


<script>

$(function(){

  var names = []
  var values = <%= tasting_note.flavor_attribute_values.to_json %>
  var options = {animation: false, pointDot: false, scaleShowLabels: false}
  new FlavorWheel.Wheel("#tasting-chart-<%= tasting_note.id %>", names, values, options).init()

})


</script>